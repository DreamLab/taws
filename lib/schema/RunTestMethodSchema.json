{
    "id": "params",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1,
            "required": true
        },
        "config": {
            "type": "array",
            "minItems": 1,
            "required": true,
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "required": true
                    },
                    "options": {
                        "type": "object"
                    },
                    "tests": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "required": true
                                },
                                "key": {
                                    "type": "string",
                                    "required": true
                                },
                                "value": {
                                    "type": "string",
                                    "required": true
                                }
                            }
                        }
                    },
                    "time": {
                        "type": "integer"
                    }
                }
            }
        }
    }
}
